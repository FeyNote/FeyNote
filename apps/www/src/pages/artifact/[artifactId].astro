---
import {
  ShareviewApp,
  trpc,
} from '@feynote/ui';

const { artifactId } = Astro.params;
---
<script>
  const artifact = await trpc.artifact.getArtifactById.query({
    id: artifactId,
  });

  console.log(artifact);
</script>
<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <title>FeyNote</title>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <link rel="icon" type="image/x-icon" href="/favicon.ico" />
  </head>
  <body>
    <div id="root"></div>
    <div id="referencePreviewContainer"></div>
    <ShareviewApp id={artifactId} client:load="react" />
  </body>
</html>
