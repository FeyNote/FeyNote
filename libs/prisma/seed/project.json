{
  "name": "prisma-seed",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/prisma/seed/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/prisma/seed",
        "main": "libs/prisma/seed/src/index.ts",
        "tsConfig": "libs/prisma/seed/tsconfig.lib.json",
        "assets": ["libs/prisma/seed/*.md"]
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/prisma/seed/jest.config.ts"
      }
    },
    "seed": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/prisma/seed",
        "command": "tsx src/lib/seed.ts"
      }
    },
    "seed:dev": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "libs/prisma/seed",
        "command": "tsx src/lib/seedDevelopment.ts"
      }
    }
  },
  "tags": []
}
